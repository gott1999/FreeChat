    <style name="Theme.UrChat.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.UrChat.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>